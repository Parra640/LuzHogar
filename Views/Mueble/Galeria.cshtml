@model List<Mueble>

<h1>Galeria de muebles</h1>

<div class="row">
@foreach (var item in Model)
{
        //info de cada mueble
        <div class="col-sm-3 border p-1" >
            <img src="@item.Foto" width="200px">
            <h3>@item.Nombre</h3>
            <p>@item.Descripcion</p>
            <a class="btn btn-primary" href="~/mueble/mueble?id=@item.Id">Mas Info</a>
        </div>

}
</div>