@model List<Mueble>

<h1>Galeria de muebles</h1>


@foreach (var item in Model)
{
    <div>
        //info de cada mueble
        <div class="sm-4 center" >
            <img src="@item.Foto" width="100px">
            <h3>@item.Nombre</h3>
            <p>@item.Descripcion</p>
            <a class="btn btn-primary" href="~/mueble/mueble?id=@item.Id">Mas Info</a>
        </div>

    </div>
}